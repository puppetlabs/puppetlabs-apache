# The Perl Apache module configuration file is being
# managed by Puppet an changes will be overwritten.

<IfModule mod_perl.c>
  <% if @perlswitches -%>
  PerlSwitches   <%= @perlswitches %>
  <% end -%>
  <% if @perlloadmodule -%>
  PerlLoadModule <%= @perlloadmodule %>
  <% end -%>
  <% if @perlsetvar -%>
  PerlSetVar     <%= @perlsetvar %>
  <% end -%>
</IfModule>
