# The Perl Apache module configuration file is being
# managed by Puppet an changes will be overwritten.

<IfModule mod_perl.c>
<% if @perl_switches -%>
  PerlSwitches <%= @perl_switches %>
<% end -%>
<% if @perl_load_module -%>
  PerlLoadModule <%= @perl_load_module %>
<% end -%>
<% if @perl_set_var -%>
  PerlSetVar <%= @perl_set_var %>
<% end -%>
</IfModule>
