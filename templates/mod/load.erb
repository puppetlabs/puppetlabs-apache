<% if @loadfiles -%>
<% Array(@loadfiles).each do |loadfile| -%>
LoadFile <%= loadfile %>
<% end -%>

<% end -%>
LoadModule <%= @_id %> <%= @_path %>
<% if @_id == 'authnz_ldap_module' && scope.lookupvar('::apache::version::distrelease') == '7' -%>
LoadModule ldap_module modules/mod_ldap.so
<% end -%>
